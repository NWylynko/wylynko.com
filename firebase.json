{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/nick",
        "destination": "https://nick.wylynko.com/",
        "type": 301
      },
      {
        "source": "/ella",
        "destination": "https://instagram.com/wylynko",
        "type": 301
      }
    ]
  }
}
